{
  "name": "cooking-ai-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cooking-ai-angular/src",
  "projectType": "application",
  "prefix": "cooking-ai-angular",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/cooking-ai-angular",
        "index": "apps/cooking-ai-angular/src/index.html",
        "browser": "apps/cooking-ai-angular/src/main.ts",
        "polyfills": ["apps/cooking-ai-angular/src/polyfills.ts"],
        "tsConfig": "apps/cooking-ai-angular/tsconfig.app.json",
        "assets": [
          "apps/cooking-ai-angular/src/favicon.ico",
          "apps/cooking-ai-angular/src/assets"
        ],
        "styles": ["apps/cooking-ai-angular/src/styles.css"],
        "scripts": [],
        "outputHashing": "none"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "outputHashing": "none"
        }
      },
      "defaultConfiguration": "production",
      "dependsOn": [
        {
          "target": "build:tailwind",
          "projects": "self",
          "params": "forward"
        }
      ]
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "cooking-ai-angular:build:production"
        },
        "development": {
          "buildTarget": "cooking-ai-angular:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "cooking-ai-angular:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "apps/cooking-ai-angular/**/*.ts",
          "apps/cooking-ai-angular/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/cooking-ai-angular/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "cooking-ai-angular:build",
        "port": 4200
      }
    }
  }
}